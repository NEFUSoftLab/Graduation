<?xml version="1.0" encoding='UTF-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
                            "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                            "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="edu.nefu.gdms.beans">
	<class name="StudentBean" table="student">
		<id name="sid" column="sid">
			<generator class="uuid"></generator>
		</id>
		<property name="number" column="number" type="java.lang.String"
			not-null="true" length="16"></property>
		<property name="pwd" column="pwd" type="java.lang.String"
			not-null="true" length="16" />
	</class>
</hibernate-mapping>
